    "husky": "^0.14.3",
    "jison-gho": "^0.6.1-215",
    "karma": "^3.0.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-source-map-support": "^1.2.0",
    "merge-options": "^1.0.0",
    "mocha": "^5.2.0",
    "source-map-support": "^0.5.6",
    "touch": "^3.1.0",
