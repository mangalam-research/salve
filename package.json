    "temp": "~0.8.0",
    "tslib": "^1.9.3",
    "xregexp": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@types/argparse": "^1.0.34",
    "@types/file-url": "^2.0.0",
    "@types/node": "^10.5.5",
    "@types/node-fetch": "^2.1.2",
    "@types/require-dir": "^0.3.0",
    "@types/temp": "^0.8.29",
    "bluebird": ">=3.1.1 <4",
    "chai": "^4.1.2",
    "child-process-promise": "^2.2.1",
    "conventional-changelog-cli": "^2.0.1",
    "del": "^3.0.0",
